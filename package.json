{
  "name": "tvmaze-scraper",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "depcheck": "./node_modules/.bin/depcheck",
    "mocha": "NODE_ENV=test && _mocha",
    "mocha_windows": "set NODE_ENV=test && _mocha",
    "check": "./node_modules/.bin/eslint ./**.js .",
    "coverage": "./node_modules/.bin/istanbul cover --print both _mocha --",
    "pretest": " npm run check && npm run depcheck",
    "test": "NODE_ENV=test && npm run coverage",
    "posttest": "./node_modules/.bin/istanbul check-coverage --statements 90 --branches 90 --functions 90",
    "start": "NODE_ENV=dev && node index.js",
    "start_windows": "set NODE_ENV=dev && node index.js",
    "debug": "NODE_ENV=test && _mocha --recursive --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh:git@github.com:rodrigocmoreira/tvmaze-scraper.git"
  },
  "keywords": [
    "tvmaze"
  ],
  "author": "Rodrigo Cardoso Moreira",
  "license": "ISC",
  "homepage": "https://github.com/rodrigocmoreira/tvmaze-scraper#readme",
  "engines": {
    "node": "8.11.1"
  },
  "dependencies": {
    "async": "^2.1.4",
    "body-parser": "^1.17.2",
    "express": "^4.14.0",
    "helmet": "^3.8.1",
    "moment": "^2.18.1",
    "nconf": "^0.10.0",
    "request": "^2.81.0",
    "sgt-fields": "^1.1.2",
    "winston": "^2.4.2",
    "express-winston": "^2.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "chance": "^1.0.11",
    "depcheck": "^0.6.6",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "nock": "^9.0.14",
    "sinon": "^3.3.0",
    "supertest": "^3.0.0"
  }
}
